package between;

import java.util.ArrayList;
import java.util.List;

/**
 * 记录发过来的最新信息，并且始终保持100条最新的记录
 * @author Admain
 *
 */
public class CurrentInfo {
	private static List list=new ArrayList<>();
	public void add(String Info){
		if(Info!=null){
			list.add(list);
		}
		if(list.size()>100){
			list.remove(list.size()-1);
		}
	}
}
