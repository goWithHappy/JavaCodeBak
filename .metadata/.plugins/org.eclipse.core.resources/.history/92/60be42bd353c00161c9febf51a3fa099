package mediator;

public class Finacial implements Department{
	private Mediator m;
	public Finacial(Mediator m) {
		this.m=m;
		m.register("department", this);
	}
	@Override
	public void selfAction() {
		System.out.println("튛킱미미");
	}

	@Override
	public void outAction() {
		System.out.println("퍬킱미미");
	}

}
