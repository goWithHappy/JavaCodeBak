package test;

/**
 * 测试java.lang.Class对象的获取方式
 * @author Admain
 *
 */
@SuppressWarnings("all")
public class Demo01 {
	public static void main(String[] args) {
		String path="bean.test";
		
		try {
			Class clazz=Class.forName(path);
			System.out.println(clazz);
		} catch (ClassNotFoundException e) {
			e.printStackTrace();
		}
		
	}

}
