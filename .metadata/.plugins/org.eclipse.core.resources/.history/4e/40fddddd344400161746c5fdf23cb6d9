		<servlet-class>FianlSever.LoginServlet</servlet-class>
	</servlet>
	<mapping>
		<servlet-mapping>login</servlet-mapping>
		<url-pattern>/login</url-pattern>
	</mapping>
	
	<servlet>
		<servlet-name>getOne</servlet-name>
		<servlet-class>FianlSever.GetOneServlet</servlet-class>
	</servlet>
	<mapping>
		<servlet-name>getOne</servlet-name>
		<url-pattern>/getOne</url-pattern>
	</mapping>
</web-app>